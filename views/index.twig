                        {% include 'mi5/richtext.twig' with {
                          label: "Bio",
                          id: "tvcontent",
                          name: "tvcontent",
                          placeholder: "Jack and Jill went up the hill."
                        } %}
                      </div>
                    </div>
                  </div>
                </details>
                <details class="light">
                  <summary>
                    <h3><span class="emoji">📓</span>&nbsp;Info</h3>
                  </summary>
