                      <div>
                        {% include 'mi5/dropdown-list-menu.twig' with {
                          options:[{
                            title:'MODX',
                            value:'modx'
                          },{
                            title:'Wordpress',
                            value:'wordpress'
                          },{
                            title:'Joomla!',
                            value:'joomla!'
                          }]
                        } %}
                      </div>
                      <div>
                        {% include './radios.twig' with {
                          legend:'Options',
                          options:[{
                            for: 'option_a',
                            id: 'option_a',
                            name: 'radio',
                            label: 'Option A',
                            type: 'radio'
                          },{
                            for: 'option_b',
                            id: 'option_b',
                            name: 'radio',
                            label: 'Option B',
                            type: 'radio'
                          },{
                            for: 'option_c',
                            id: 'option_c',
                            name: 'radio',
                            label: 'Option C',
                            type: 'radio'
                          },{
                            for: 'option_d',
                            id: 'option_d',
                            name: 'radio',
                            label: 'Option D',
                            type: 'radio'
