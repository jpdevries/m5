                          title:'mail@devries.jp'
                        },{
                          title:'johnpaul.devries@gmail.com'
                        }]
                        } %}
                      </div>
                      <div>
                        {% include 'mi5/labeledinput.twig' with {
                          id:'tel',
                          name:'tel',
                          type:'tel',
                          label:'Cell',
                          placeholder:'(555) 555–5555'
                        } %}
                      </div>
                      <div>
                        {% include 'mi5/url.twig' with {
                        placeholder:'https://modx.com',
                          options:[{
                            title:'http://google.com'
                          },{
                            title:'http://facebook.com'
                          }]
                        } %}
