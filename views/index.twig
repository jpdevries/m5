                            id: 'option_f',
                            name: 'radio',
                            label: 'Option F',
                            type: 'radio'
                          },{
                            for: 'option_g',
                            id: 'option_g',
                            name: 'radio',
                            label: 'Option G',
                            type: 'radio'
                          },{
                            for: 'option_h',
                            id: 'option_h',
                            name: 'radio',
                            label: 'Option H',
                            type: 'radio'
                          }]
                        } %}
                      </div>
                    </div>
                  </div>
