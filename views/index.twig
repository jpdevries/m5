                    <div>
                      <div>
                        {% include 'mi5/listbox-multi.twig' %}
                      </div>
                      <div>
                        {% include 'mi5/slider.twig' with {
                          min:0,
                          max:200,
                          step:1,
                          options:[{
                            title:100
                          },{
                          title:150
                          }]
                        } %}
                      </div>
                      <div>
                        {% include 'mi5/file.twig' %}
                      </div>
                      <div>
                        {% include 'mi5/dropdown-list-menu.twig' with {
                          id: 'airports',
                          name: 'airports',
                          label: 'Airports',
                          options:[{
                            label:'California',
                            options:[{
                              title:'Sacramento',
                              value:'SAC'
                            },{
                              title:'Los Angeles',
                              value:'LAX'
                            },{
                              title:'San Francisco',
                              value:'SFO'
                            }]
                          },{
                            label:'Oregon',
                            options:[{
                              title:'Portland',
                              value:'PDX'
                            }]
                          }]
                        } %}
