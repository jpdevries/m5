{% include 'header.twig' with { loadStyles:true, treeless:true } %}
</head>
<body class=""><!-- .snappy -->
  {% if deferStyles|default(false) %}<link rel="stylesheet" href="assets/css/main{% if production %}.min{% endif %}.css" media="screen" title="Revolution Manager Theme">{% endif %}
  <div class="page-wrapper">
    <aside class="sidenav">
      <header id="mainnav">
        <div class="page">
        <h1>MODX Advisory Board</h1>
        <h2 class="version">
          <a>
            <svg class="logo">
              <title>Dashboard</title>
              <use xlink:href="#modx"></use>
              <!--use xlink:href="assets/img/icons.svg#modx"></use--> <!-- http://stackoverflow.com/a/27752594/4671250 -->
            </svg>
          </a>
          <span>MODX <code>4.0.0-pl</code></span>
        </h2>
        </div>
        <form id="uberform">
          <div>
            <input placeholder="context:web blog" type="text" id="uber" name="q" required />
            <label for="uber"><h1><span emoji>🔎</span> Search<span visually-hidden> for anything&hellip;</span></h1></label>
          </div>
          <div class="radio-bar">
            <fieldset>
              <legend>Hide Unmatched Elements</legend>
              <div>
                <input type="radio" id="hide_unmatched_elements__yes" name="hide_unmatched_elements" value="1" />
                <label for="hide_unmatched_elements__yes" checked>Yes</label>

                <input type="radio" id="hide_unmatched_elements__no" name="hide_unmatched_elements" value="0" checked />
                <label for="hide_unmatched_elements__no">No</label>
              </div>
            </fieldset>
            <div class="close-nav" hidden>
              <h3><a><span emoji>❌</span>&ensp;Close Navigation</a></h3>
            </div>
          </div>
          <!--div class="radio-bar sticky-scroll" style="margin-top:1em">
            <input type="checkbox" id="snap-scroll" name="snap-scroll">
            <label for="snap-scroll">Assistive Scrolling</label>
          </div-->
        </form>
        <!--div class="button-row">
          <div><a>New Resource</a></div>
          <div><a>New WebLink</a></div>
          <div><a>New Symlink</a></div>
          <div><a>New Static Resource</a></div>
          <div><a>Remove Deleted Resources</a></div>
        </div-->
        <nav id="mainnav__nav">
          <details>
            <summary>
              <h2>
                <!--span emoji data-iconset="svgawesome" data-icon="heart" data-svg-class="foo">📇</span--><span emoji>📇</span>&nbsp;Content
              </h2>
            </summary>
            <nav>
              <ul>
              <li class="new_resource" data-tags="new resources">
              <h3><a>📝&nbsp;New Resource</a></h3>
                <form action="/create/resource" target="_blank">
                  <label for="new_pagetitle">Create a new Resource &mdash; usually a web page</label>
                  <div class="">
                    <div class="">
                      <input required id="new_pagetitle" name="new_pagetitle" type="text" name="new_pagetitle" placeholder="blog > modResource * 3">
                    </div>
                    <div>
                      <button type="submit">Create</button>
                    </div>
                  </div>
                </form>
              </li>

              <li>
              <h3><a>👀&nbsp;Preview Site</a></h3>
              <p>View your website in a new&nbsp;window</p>
              </li>
              <li>
              <h3><a>📄&nbsp;Import HTML</a></h3>
              <p>Import HTML files to&nbsp;Resources</p>
              </li>
              <li>
              <h3><a>🔗&nbsp;Import Static Resources</a></h3>
              <p>Import any Content Type based on file extensions to Static&nbsp;Resources</p>
              </li>
              <li>
              <h3><a>📑&nbsp;Resource Groups</a></h3>
              <p>Assign Resources to&nbsp;Group</p>
              </li>
              <li>
              <h3><a>📦&nbsp;Content Types</a></h3>
              <p>Add content types for resources, such as .html, js,&nbsp;etc.</p>
              </li>
              </ul>
            </nav>
          </details>
          <details>
            <summary>
              <h2><span emoji>🎑</span>&nbsp;Media</h2>
            </summary>
            <nav>
                <ul>
                  <li>
                    <h3><a>📡&nbsp;Media Sources</a></h3>
                    <p>Media sources for use of media from remote servers or&nbsp;services</p>
                  </li>
                  <li>
                    <h3><a>👀&nbsp;Media Browser</a></h3>
                    <p>View, upload, and manage&nbsp;media</p>
                  </li>
                  <li>
                    <h3><a>🌲&nbsp;Eureka</a></h3>
                    <p>Browse your Media Sources with the Eureka Media Browser by&nbsp;mod<strong>more</strong></p>
                  </li>
                  <li class="quick-upload">
                    <h3><a>📤&nbsp;Upload</a></h3>
                    <p>Quickly upload a&nbsp;file</p>
                    <form>
                        <div>
                          <input required type="file" name="pic" accept="image/*">
                          <button type="submit">Upload</button>
                        </div>
                        <div class="dragdrop">
                        </div>
                    </form>
                  </li>
                </ul>
              </nav>
          </details>
          <details>
            <summary><h2><span emoji>🚀</span>&nbsp;Extras</h2></summary>
            <nav>
              <ul>
                <li class="extras command-line">
                  <h3><a>💥&nbsp;Installer</a></h3>
                  <p>Manage Add&ndash;ons and&nbsp;Distributions</p>
                  <textarea name="extras_commandline" rows="3" cols="32" placeholder="$ modx install getResources"></textarea>
                  <p>
                    You have 3 package updates available.<br><a>Update packages now</a>.
                  </p>
                </li>
                <li>
                  <h3><a>📑&nbsp;Collection views</a></h3>
                  <p>Define views for collection's children&nbsp;grid</p>
                </li>
                <li>
                  <h3><a>🔳&nbsp;Content Blocks</a></h3>
                  <p>Manage the Content Blocks fields and&nbsp;layouts</p>
                </li>
                <li>
                  <h3><a>↪&nbsp;Redirector</a></h3>
                  <p>Manage your&nbsp;redirects</p>
                </li>

              </ul>
            </nav>
          </details>
          <details>
            <summary>
              <h2><span emoji>💼</span>&nbsp;Manage</h2>
            </summary>
            <nav>
              <ul>
                <li>
                  <h3><a>👥&nbsp;Users</a></h3>
                  <p>Manage Users and their&nbsp;Permissions</p>
                </li>
                <li>
                  <h3><a>🚫&nbsp;Clear Cache</a></h3>
                  <p>Delete Cache files in all&nbsp;Contexts</p>
                </li>
                <li>
                  <h3><a>🔓&nbsp;Remove Locks</a></h3>
                  <p>Remove all locks on Manager&nbsp;pages</p>
                </li>
                <li>
                  <h3><a>🚽&nbsp;Flush Your Permissions</a></h3>
                  <p>Reload this session's&nbsp;Permissions</p>
                </li>
                <li>
                  <h3><a>⛔&nbsp;Lougout All Users</a></h3>
                  <p>Immediately destroy all&nbsp;sessions</p>
                </li>
                <li>
                  <h3><a>📈&nbsp;Reports</a></h3>
                  <p>Admin reports for your MODX&nbsp;install</p>
                </li>
