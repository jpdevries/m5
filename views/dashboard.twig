    <main>
      <div class="dashboard wrapper">
        <div class="box a">
          <h2>Recently Edited Resources</h2>
        </div>
        <div class="document box b">
          <h2>Quick Create Resource</h3>
            <form class="" action="index.html" method="post">
              <label for="pagetitle">Pagetitle</label>
              <input type="text" name="pagetitle" id="pagetitle" value="" placeholder="Another new post">
              <button type="submit">Create</button>
            </form>
        </div>
